<%- if controller_name != 'sessions' %>
  <%= link_to "Залогуватися", new_session_path(resource_name), { class: "btn btn-primary" } %><br />
<% end -%>

<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <%= link_to "Зареєструватися", new_registration_path(resource_name), { class: "btn btn-primary" } %><br />
<% end -%>

